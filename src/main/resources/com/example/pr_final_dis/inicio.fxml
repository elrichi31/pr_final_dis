<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.example.pr_final_dis.InicioController"
          hgap="10" vgap="10">
    <Label text="Identificador del cliente:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <TextField fx:id="txtIdentificador" GridPane.columnIndex="1" GridPane.rowIndex="0" />
